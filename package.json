{
	"$schema": "https://www.schemastore.org/package.json",
	"packageManager": "pnpm@10.0.0",
	"name": "turns",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"biome": "biome check --write",
		"prettier": "prettier --write .",
		"check-biome": "biome check",
		"check-prettier": "prettier .",
		"check-superhtml": "superhtml fmt --check index.html",
		"check-types": "tsc --noEmit",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"superhtml": "superhtml fmt index.html",
		"storybook": "storybook dev --no-open",
		"test": "vitest run",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/svelte-vite": "^10.0.8",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.1",
		"@webgpu/types": "^0.1.68",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"storybook": "^10.0.8",
		"svelte": "^5.43.14",
		"turbo": "^2.6.1",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.1.14",
		"vitest": "^4.0.13"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.1.14"
		}
	}
}
